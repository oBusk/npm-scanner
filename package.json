{
    "name": "@obusk/npm-scanner",
    "private": true,
    "version": "0.1.0",
    "description": "Basic virus scanning for npm packages",
    "keywords": [
        "npm",
        "virustotal",
        "nextjs",
        "scanner"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/oBusk/npm-scanner.git"
    },
    "author": "Oscar Busk <oscar.busk@gmail.com>",
    "license": "ISC",
    "engines": {
        "node": "14.x",
        "npm": "8.x"
    },
    "volta": {
        "node": "14.18.1",
        "npm": "8.1.3"
    },
    "simple-git-hooks": {
        "pre-commit": "npm run lint-staged"
    },
    "scripts": {
        "dev": "next",
        "build": "next build",
        "start": "next start",
        "type-check": "tsc",
        "lint": "next lint --ignore-path .gitignore",
        "postlint": "npm run prettier",
        "lint-fix": "next lint --ignore-path .gitignore --fix",
        "prettier": "prettier **/*.{md,yml,yaml,json} --ignore-path .gitignore --check",
        "postlint-fix": "npm run prettier-fix",
        "prettier-fix": "prettier **/*.{md,yml,yaml,json} --ignore-path .gitignore --write",
        "lint-staged": "lint-staged"
    },
    "dependencies": {
        "@chakra-ui/react": "^1.7.0",
        "@emotion/react": "^11.5.0",
        "@emotion/styled": "^11.3.0",
        "framer-motion": "^5.2.1",
        "next": "^12.0.3",
        "pacote": "^12.0.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-icons": "^4.3.1"
    },
    "devDependencies": {
        "@types/jest": "^27.0.2",
        "@types/pacote": "^11.1.1",
        "@types/react": "^17.0.34",
        "@types/react-dom": "^17.0.11",
        "eslint": "^7.32.0",
        "eslint-config-next": "^12.0.3",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-prettier": "^4.0.0",
        "lint-staged": "^11.2.6",
        "prettier": "^2.4.1",
        "simple-git-hooks": "^2.7.0",
        "typescript": "4.4.4"
    }
}
